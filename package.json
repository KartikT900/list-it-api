{
  "name": "list-it-api",
  "version": "1.0.0",
  "description": "API for List-IT-UI",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "lint": "eslint --ext .js --ext .ts ./src",
    "format": "prettier --check --write  \"src/**/*.ts\"",
    "start": "node --trace-warnings --es-module-specifier-resolution=node dist/src/index",
    "dev": "nodemon ./src/index.ts",
    "build": "npx tsc --project tsconfig.build.json",
    "dev:test": "jest --detectOpenHandles --coverage --watchAll",
    "test": "jest --coverage --silent --watchAll",
    "test:ci": "jest --bail --coverage=true",
    "prepare:dev": "husky install",
    "prisma:generate": "npx prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KartikT900/list-it-api.git"
  },
  "keywords": [
    "javascript",
    "prisma",
    "js",
    "typescript"
  ],
  "author": "KartikT900",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KartikT900/list-it-api/issues"
  },
  "homepage": "https://github.com/KartikT900/list-it-api#readme",
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "@types/swagger-ui-express": "^4.1.3",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "4.18.2",
    "express-jwt": "^7.7.7",
    "express-session": "1.17.3",
    "http-status": "1.5.3",
    "jwks-rsa": "^2.1.5",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "swagger-ui-express": "^4.5.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/jest": "29.1.2",
    "@types/node": "18.11.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "29.2.0",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prisma": "^4.4.0",
    "supertest": "6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "type": "module"
}
