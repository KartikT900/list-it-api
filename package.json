{
  "name": "list-it-api",
  "version": "1.0.0",
  "description": "API for List-IT-UI",
  "main": "index.js",
  "scripts": {
    "lint-staged": "lint-staged",
    "lint": "eslint --fix",
    "format": "prettier --check --write  \"src/**/*.ts\"",
    "start": "node index",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "dev:test": "jest --detectOpenHandles --coverage --watchAll",
    "test": "jest --coverage --silent",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KartikT900/list-it-api.git"
  },
  "keywords": [
    "javascript",
    "prisma",
    "js",
    "typescript"
  ],
  "author": "KartikT900",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KartikT900/list-it-api/issues"
  },
  "homepage": "https://github.com/KartikT900/list-it-api#readme",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "http-status": "1.5.3",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/jest": "29.1.2",
    "@types/node": "18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "29.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "supertest": "6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.8.4"
  }
